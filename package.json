{
  "name": "dumi-theme-antd-mobile",
  "version": "0.0.1",
  "description": "A antd-mobile site theme package for the dumi framework.",
  "types": "dist/types.d.ts",
  "scripts": {
    "dev": "APP_ROOT=example dumi dev",
    "build": "father build",
    "prepare": "husky install && father link-dev-theme && APP_ROOT=example dumi setup",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepublishOnly": "father doctor && npm run build"
  },
  "keywords": [
    "dumi",
    "dumi theme",
    "mobile library"
  ],
  "authors": [
    "1395093509@qq.com"
  ],
  "license": "MIT",
  "sideEffects": [
    "**/*.less"
  ],
  "files": [
    "dist"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "dumi": "^2.3.0",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@umijs/lint": "^4.0.0",
    "dumi": "^2.3.0",
    "eslint": "^8.23.0",
    "father": "^4.1.0",
    "father-plugin-dumi-theme": "^1.0.0-rc.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "stylelint": "^14.9.1"
  },
  "dependencies": {
    "@ant-design/icons-svg": "^4.2.1",
    "f2-touchemulator": "^0.0.1",
    "@ant-design/icons": "^5.4.0",
    "ahooks": "^3.8.0",
    "antd": "^4.24.16",
    "antd-mobile": "^5.37.1",
    "antd-mobile-icons": "^0.3.0",
    "classnames": "^2.5.1",
    "lz-string": "^1.5.0",
    "prism-react-renderer": "^2.3.1",
    "prismjs": "^1.29.0",
    "qrcode.react": "^3.1.0",
    "react-lottie": "^1.2.4",
    "umi-hd": "^5.0.1"
  }
}
